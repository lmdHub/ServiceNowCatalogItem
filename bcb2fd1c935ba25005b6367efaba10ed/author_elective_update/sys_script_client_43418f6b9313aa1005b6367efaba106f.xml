        <messages/>
        <name>AutoPopulate_UserDetails</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

   var userDetails = g_form.getValue('user_id');

   var ga = new GlideAjax (userDetails);
   ga.addParam('sysparm_name', 'getUserdetails');
   ga.addParam('sysparm_value', userDetails);
   ga.getXML(callback);

   function callback(response){
	var answer = response.responseXML.documentElement.getAttribute('answer');
	var test = answer.split(',');

	console.log(test[1]);

	g_form.setValue('email_address', test[0]);
	g_form.setValue('company_name', test[1]);
	g_form.setValue('comp_location', test[2]);
   }

   //Type appropriate comment here, and begin script below
   
}]]></script>
